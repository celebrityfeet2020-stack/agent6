# M3 Agent System v6.0 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# ============================================
# LLM配置
# ============================================
LLM_API_KEY=your_api_key_here
LLM_BASE_URL=https://api.openai.com/v1
LLM_MODEL=gpt-4

# ============================================
# Agent配置
# ============================================
AGENT_NAME=M3 Agent
AGENT_VERSION=6.0.0
DEFAULT_THREAD_ID=default_session

# ============================================
# 浏览器配置
# ============================================
BROWSER_HEADLESS=true
BROWSER_POOL_SIZE=3

# ============================================
# 日志配置
# ============================================
LOG_LEVEL=INFO

# ============================================
# 时区配置
# ============================================
TZ=Asia/Shanghai
